<Page x:Class="Ultimative.MCL.Pages.PageSetGeneral"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mclpages="clr-namespace:Ultimative.MCL.Pages"
      mc:Ignorable="d"
      d:DesignHeight="450"
      d:DesignWidth="800"
      Title="PageSetGeneral">
    <ScrollViewer VerticalScrollBarVisibility="Disabled">
        <StackPanel Margin="40,0">
            <HeaderedContentControl Header="Ultimative Universal"
                                    FontSize="14">
                <StackPanel Background="{DynamicResource BackgroundTier3Brush}">
                    
                </StackPanel>
            </HeaderedContentControl>
        </StackPanel>
        
    </ScrollViewer>
</Page>
