                <ResourceDictionary Source="/Ultimative.Universal;component/ResourceDictionaries/Ultimative.TextBlocks.xaml" />
                <ResourceDictionary Source="/Ultimative.Universal;component/ResourceDictionaries/Ultimative.ScrollViewers.xaml" />
                <ResourceDictionary Source="/Ultimative.Universal;component/ResourceDictionaries/Ultimative.TabControls.xaml" />
                <!--ResourceDictionary Source="/Ultimative.Universal;component/ResourceDictionaries/Ultimative.ModernUI.xaml" /-->
                <modern:ThemeResources />
                <modern:XamlControlsResources />
